{
	"name": "classic-theme",
	"version": "1.0.0",
	"description": "Classic Theme Packages",
	"main": "Gruntfile.js",
	"scripts": {
		"build": "npm run clean && cross-env NODE_ENV=production webpack --mode production --progress",
		"start": "npm run clean && cross-env NODE_ENV=development webpack --watch --mode development --progress",
		"clean": "rimraf assets/build/*",
		"init": "node ./bin/init.js && npm run build:pot && npm run build:zip",
		"build:pot": "grunt i18n",
		"build:zip": "grunt release",
		"lint:all": "npm-run-all --parallel lint:*",
		"lint:css:fix": "npm run lint:css -- --fix ./assets/src",
		"lint:js": "wp-scripts lint-js ./assets/src",
		"lint:js:fix": "npm run lint:js -- --fix ./assets/src",
		"lint:js:report": "npm run lint:js -- --output-file lint-js-report.json --format json .",
		"lint:php": "php vendor/bin/phpcs",
		"lint:php:fix": "node ./bin/phpcbf.js",
		"lint:staged": "lint-staged",
		"precommit": "npm-run-all lint:* pot",
		"cypress:open": "cypress open",
		"prepare": "npm run init",
		"wp-env": "wp-env"
	},
	"keywords": [
		"wordpress",
		"rabindratharu",
		"wp-theme",
		"classic-theme"
	],
	"homepage": "https://github.com/rabindratharu/classic-theme#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/rabindratharu/classic-theme.git"
	},
	"bugs": {
		"url": "https://github.com/rabindratharu/classic-theme/issues"
	},
	"author": "rabindratharu",
	"license": "GPL-2.0-or-later",
	"private": true,
	"devDependencies": {
		"@babel/core": "^7.27.1",
		"@babel/preset-env": "^7.27.1",
		"@babel/preset-react": "^7.27.1",
		"@svgr/cli": "^8.1.0",
		"@wordpress/base-styles": "^5.25.0",
		"@wordpress/block-editor": "^14.17.0",
		"@wordpress/blocks": "^14.11.0",
		"@wordpress/components": "^29.8.0",
		"@wordpress/compose": "^7.25.0",
		"@wordpress/data": "^10.25.0",
		"@wordpress/dependency-extraction-webpack-plugin": "^6.25.0",
		"@wordpress/element": "^6.25.0",
		"@wordpress/eslint-plugin": "^22.8.0",
		"@wordpress/hooks": "^4.25.0",
		"@wordpress/i18n": "^5.25.0",
		"@wordpress/icons": "^10.25.0",
		"@wordpress/server-side-render": "^5.25.0",
		"autoprefixer": "^10.4.21",
		"babel-loader": "^10.0.0",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^13.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^7.1.2",
		"css-minimizer-webpack-plugin": "^7.0.2",
		"cypress": "^14.3.2",
		"eslint": "^9.26.0",
		"eslint-plugin-jsdoc": "^50.6.11",
		"eslint-webpack-plugin": "^5.0.1",
		"grunt": "^1.6.1",
		"grunt-checktextdomain": "^1.0.1",
		"grunt-contrib-clean": "^2.0.1",
		"grunt-contrib-compress": "^2.0.0",
		"grunt-contrib-copy": "^1.0.0",
		"grunt-wp-i18n": "^1.0.3",
		"grunt-wp-readme-to-markdown": "^2.1.0",
		"lodash": "^4.17.21",
		"mini-css-extract-plugin": "^2.9.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.5.3",
		"postcss-loader": "^8.1.1",
		"postcss-preset-env": "^10.1.3",
		"rimraf": "^6.0.1",
		"sass": "^1.87.0",
		"sass-loader": "^16.0.5",
		"sass-mq": "^6.0.0",
		"style-loader": "^4.0.0",
		"stylelint": "^16.19.1",
		"stylelint-config-standard-scss": "^14.0.0",
		"stylelint-webpack-plugin": "^5.0.1",
		"webpack": "^5.99.7",
		"webpack-cli": "^6.0.1",
		"webpack-remove-empty-scripts": "^1.0.4",
		"rtlcss": "^4.3.0",
		"rtlcss-webpack-plugin": "^4.0.7",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"jest-silent-reporter": "^0.6.0",
		"lint-staged": "^15.4.3"
	},
	"browserslist": {
		"production": [
			"> 0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {}
}