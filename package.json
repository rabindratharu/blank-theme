{
	"name": "blank-theme",
	"version": "1.0.0",
	"homepage": "https://github.com/rabindratharu/blank-theme",
	"scripts": {
		"build:dev": "npm run clean && wp-scripts start --webpack-src-dir=assets/src --output-path=assets/build --webpack-copy-php",
		"build:prod": "npm run clean && wp-scripts build --webpack-src-dir=assets/src --output-path=assets/build --webpack-copy-php",
		"clean": "rm -rf assets/build/*",
		"init": "npm run build:pot && npm run build:zip",
		"build:pot": "grunt i18n",
		"build:zip": "grunt release",
		"start": "npm run build:dev",
		"build": "npm run build:prod"
	},
	"devDependencies": {
		"@babel/core": "^7.26.10",
		"@babel/preset-env": "^7.26.9",
		"@wordpress/block-editor": "^14.6.0",
		"@wordpress/blocks": "^14.0.0",
		"@wordpress/components": "^28.11.0",
		"@wordpress/core-data": "^7.11.0",
		"@wordpress/dom-ready": "^4.11.0",
		"@wordpress/element": "^6.11.0",
		"@wordpress/hooks": "^4.11.0",
		"@wordpress/i18n": "^5.11.0",
		"@wordpress/rich-text": "^7.11.0",
		"@wordpress/scripts": "^30.7.0",
		"@wordpress/token-list": "^3.11.0",
		"autoprefixer": "^10.4.20",
		"babel-loader": "^10.0.0",
		"classnames": "^2.5.1",
		"copy-webpack-plugin": "^12.0.2",
		"css-loader": "^7.1.2",
		"css-minimizer-webpack-plugin": "^7.0.0",
		"glob": "^11.0.0",
		"grunt-checktextdomain": "^1.0.1",
		"grunt-contrib-clean": "^2.0.1",
		"grunt-contrib-compress": "^2.0.0",
		"grunt-contrib-copy": "^1.0.0",
		"grunt-wp-i18n": "^1.0.3",
		"grunt-wp-readme-to-markdown": "^2.1.0",
		"mini-css-extract-plugin": "^2.9.2",
		"npm-check-updates": "^17.1.9",
		"npm-run-all": "^4.1.5",
		"path": "^0.12.7",
		"postcss": "^8.4.49",
		"postcss-loader": "^8.1.1",
		"sass-loader": "^16.0.5",
		"tailwindcss": "^3.4.15",
		"webpack-remove-empty-scripts": "^1.0.4",
		"zustand": "^5.0.2"
	},
	"dependencies": {
		"@wordpress/icons": "^10.11.0",
		"@wordpress/interactivity": "^6.14.0",
		"axios": "^1.7.9",
		"dompurify": "^3.2.4",
		"fs": "^0.0.1-security",
		"magnific-popup": "^1.2.0",
		"rtlcss-webpack-plugin": "^4.0.7"
	},
	"browserslist": [
		"last 3 versions",
		"> 1%"
	]
}
